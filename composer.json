{
    "name": "bmc/openexam-phalcon-mvc",
    "version": "2.2.0",
    "description": "Online system for electronic exams (CBE).",
    "repositories": [
        {
            "type": "composer",
            "url": "https://it.bmc.uu.se/andlov/php/proj/bmc/uup-auth/"
        },
        {
            "type": "composer",
            "url": "https://it.bmc.uu.se/andlov/php/proj/bmc/uup-mail/"
        }
    ],
    "authors": [
        {
            "name": "Anders LÃ¶vgren",
            "email": "anders.lovgren@bmc.uu.se"
        },
        {
            "name": "Ahsan Shahzad",
            "email": "ahsan.shahzad@medfarm.uu.se"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "bmc/uup-auth": ">=2.3.10",
        "bmc/uup-mail": ">=1.0.4",
        "phalcon/devtools": "^2.0",
        "phalcon/incubator": "^2.0",
        "phpoffice/phpexcel": "~1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "bartlett/php-compatinfo": "For static code compatibility checks.",
        "jpgraph/jpgraph": "For statistical pages or dynamic generated GUI components, e.g. buttons.",
        "piotrooo/wsdl-creator": "Create WSDL document based on PHP classes using annotations and reflection mechanism."
    },
    "autoload": {
        "psr-4": {
            "OpenExam\\Composer\\": "admin/composer"
        }
    },
    "scripts": {
        "post-package-install": [
            "OpenExam\\Composer\\Installer::postPackageInstall"
        ],
        "post-package-update": [
            "OpenExam\\Composer\\Installer::postPackageUpdate"
        ]
    }
}
