            "company": "Computing Department at BMC, Uppsala University"
        },
        {
            "name": "Ahsan Shahzad",
            "email": "ahsan.shahzad@medfarm.uu.se",
            "role": "Developer",
            "company": "Medfarm DoIT, Uppsala University"
        },
        {
            "name": "Anders Lövgren",
            "email": "ipv6nu@gmail.com",
            "role": "Developer",
            "company": "Nowise Systems"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "bmc/uup-auth": ">=2.3.17",
        "bmc/uup-mail": ">=1.0.4",
        "phalcon/incubator": "^3.2",
        "phpoffice/phpexcel": "~1.8"
    },
    "require-dev": {
